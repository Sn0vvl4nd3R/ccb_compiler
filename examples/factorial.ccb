// Вычисление факториала числа 5
// Это главная часть программы

let n = 5; // Число, для которого ищем факториал
let result = 1; // Здесь, хранится результат
let i = 1; // Счетчик для цикла

out 1001; // Сигнал: начинаем вычисление

while (i < n + 1) {
  result = result * i;
  i = i + 1;
}

out 1002; // Сигнал: вычисление завершено
out result;
